{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nimport { get, clear, set, del, keys, entries } from 'idb-keyval';\nexport default {\n  name: 'HelloWorld',\n  data() {\n    return {\n      resp: '',\n      data_tasks: '',\n      currIp: this.currIp\n    };\n  },\n  methods: {\n    logout() {\n      if (confirm(\"Êtes-vous sûr de vouloir vous déconnecter ? Vous perdrez votre avancement sur les tâches en cours.\")) {\n        clear();\n        this.$router.push('/login/');\n      }\n    },\n    reloadPage() {\n      window.location.reload();\n    }\n  },\n  async mounted() {\n    let current_tech = await get('current_tech');\n    if (typeof current_tech == 'undefined') {\n      this.$router.push('/login/');\n    } else {\n      const sendGetRequest = async () => {\n        try {\n          const response = await axios.get(this.currIp + '/my_missions?tech_id=' + current_tech);\n          this.resp = response.data;\n\n          //del old tasks\n          const ks = await keys();\n          for (const k of ks) {\n            if (k.toString().includes(\"-task\")) {\n              del(k);\n            }\n          }\n          console.log(this.resp);\n          this.resp.forEach(async function (element) {\n            var id = element.Item_id;\n            var type = element.Item_type;\n            element = JSON.stringify(element);\n            var data = JSON.parse(element);\n\n            //set new tasks\n            await set(type + '-' + id + '-task', data);\n          });\n        } catch (err) {\n          console.error(err);\n        }\n      };\n      await sendGetRequest();\n\n      //get tasks from cache\n      var data_tasks = [];\n      var cache_entries = await entries();\n      for (var i = 0; i <= cache_entries.length - 1; i++) {\n        if (cache_entries[i][0].toString().includes(\"-task\")) {\n          //add formated_date\n          var dateParts = cache_entries[i][1][\"Evt_dtDebut\"].split(\" \");\n          dateParts = dateParts[1].split(\"/\");\n          var dateObject = new Date(+dateParts[2], dateParts[1] - 1, +dateParts[0]);\n          cache_entries[i][1][\"unix_date\"] = Date.parse(dateObject);\n          data_tasks.push(cache_entries[i][1]);\n        }\n      }\n      console.log(data_tasks);\n      data_tasks.sort(function (a, b) {\n        return a[\"unix_date\"] - b[\"unix_date\"];\n      });\n      this.data_tasks = data_tasks;\n    }\n  }\n};","map":{"version":3,"mappings":";AACA,OAAOA,KAAI,MAAO,OAAM;AACxB,SAASC,GAAG,EAAEC,KAAK,EAAEC,GAAG,EAAEC,GAAG,EAAEC,IAAI,EAAEC,OAAM,QAAS,YAAY;AAEhE,eAAe;EACXC,IAAI,EAAE,YAAY;EAClBC,IAAI,GAAG;IACH,OAAO;MACHC,IAAI,EAAE,EAAE;MACRC,UAAS,EAAI,EAAE;MACfC,MAAM,EAAE,IAAI,CAACA;IACjB,CAAC;EACL,CAAC;EACDC,OAAO,EAAE;IACLC,MAAM,GAAG;MACL,IAAGC,OAAO,CAAC,oGAAoG,CAAC,EAAC;QAC7GZ,KAAK,EAAE;QACP,IAAI,CAACa,OAAO,CAACC,IAAI,CAAC,SAAS,CAAC;MAChC;IACJ,CAAC;IACDC,UAAU,GAAG;MACTC,MAAM,CAACC,QAAQ,CAACC,MAAM,EAAE;IAC5B;EACJ,CAAC;EACD,MAAMC,OAAM,GAAK;IACb,IAAIC,YAAW,GAAI,MAAMrB,GAAG,CAAC,cAAc,CAAC;IAC5C,IAAI,OAAOqB,YAAW,IAAK,WAAW,EAAE;MACpC,IAAI,CAACP,OAAO,CAACC,IAAI,CAAC,SAAS,CAAC;IAChC,OAAO;MACH,MAAMO,cAAa,GAAI,YAAY;QAC/B,IAAI;UACA,MAAMC,QAAO,GAAI,MAAMxB,KAAK,CAACC,GAAG,CAAC,IAAI,CAACU,MAAM,GAAC,uBAAuB,GAACW,YAAY,CAAC;UAClF,IAAI,CAACb,IAAG,GAAIe,QAAQ,CAAChB,IAAI;;UAEzB;UACA,MAAMiB,EAAC,GAAI,MAAMpB,IAAI,EAAE;UACvB,KAAK,MAAMqB,KAAKD,EAAE,EAAE;YAChB,IAAIC,CAAC,CAACC,QAAQ,EAAE,CAACC,QAAQ,CAAC,OAAO,CAAC,EAAE;cAChCxB,GAAG,CAACsB,CAAC,CAAC;YACV;UACJ;UAEAG,OAAO,CAACC,GAAG,CAAC,IAAI,CAACrB,IAAI,CAAC;UAEtB,IAAI,CAACA,IAAI,CAACsB,OAAO,CAAC,gBAAeC,OAAO,EAAE;YACtC,IAAIC,EAAC,GAAID,OAAO,CAACE,OAAO;YACxB,IAAIC,IAAG,GAAIH,OAAO,CAACI,SAAS;YAC5BJ,OAAM,GAAIK,IAAI,CAACC,SAAS,CAACN,OAAO,CAAC;YACjC,IAAIxB,IAAG,GAAI6B,IAAI,CAACE,KAAK,CAACP,OAAO,CAAC;;YAE9B;YACA,MAAM7B,GAAG,CAACgC,IAAI,GAAC,GAAG,GAACF,EAAE,GAAC,OAAO,EAAEzB,IAAI,CAAC;UACxC,CAAC,CAAC;QACN,EAAE,OAAOgC,GAAG,EAAE;UACVX,OAAO,CAACY,KAAK,CAACD,GAAG,CAAC;QACtB;MACJ,CAAC;MACD,MAAMjB,cAAc,EAAE;;MAEtB;MACA,I