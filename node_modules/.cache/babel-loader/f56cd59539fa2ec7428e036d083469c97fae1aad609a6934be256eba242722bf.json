r_wc[0][\"inter_type\"] != '') {\n        this.isShow_wc = true;\n      }\n      if (savedData.inter_robinetterie[0][\"inter_type\"] != '') {\n        this.isShow_robinetterie = true;\n      }\n      if (savedData.inter_robinetterie_c[0][\"inter_type\"] != '') {\n        this.isShow_robinetterie_c = true;\n      }\n      this.appartements = savedData;\n    },\n    addItemRa() {\n      //mettre type en arg\n      // this['inter_'+type].push({ name: \"\", email: \"\"});\n      this.appartements.inter_ra.push({\n        inter_type: '',\n        inter_lieu: ''\n      });\n      this.appartements.repa_ra.push({\n        repa_quantity: 0,\n        repa_piece: ''\n      });\n      this.saveFormData();\n    },\n    addItemWc() {\n      //mettre type en arg\n      // this['inter_'+type].push({ name: \"\", email: \"\"});\n      this.appartements.inter_wc.push({\n        inter_type: '',\n        inter_lieu: ''\n      });\n      this.appartements.repa_wc.push({\n        repa_quantity: 0,\n        repa_piece: ''\n      });\n      this.saveFormData();\n    },\n    addItemRobinetterie() {\n      //mettre type en arg\n      // this['inter_'+type].push({ name: \"\", email: \"\"});\n      this.appartements.inter_robinetterie.push({\n        inter_type: '',\n        inter_lieu: ''\n      });\n      this.appartements.repa_robinetterie.push({\n        repa_quantity: 0,\n        repa_piece: ''\n      });\n      this.saveFormData();\n    },\n    addItemRobinetterieC() {\n      //mettre type en arg\n      // this['inter_'+type].push({ name: \"\", email: \"\"});\n      this.appartements.inter_robinetterie_c.push({\n        inter_type: '',\n        inter_lieu: ''\n      });\n      this.appartements.repa_robinetterie_c.push({\n        repa_quantity: 0,\n        repa_piece: ''\n      });\n      this.saveFormData();\n    },\n    removeItemRa() {\n      this.appartements.inter_ra.pop();\n      this.appartements.repa_ra.pop();\n    },\n    removeItemWc() {\n      this.appartements.inter_wc.pop();\n      this.appartements.repa_wc.pop();\n    },\n    removeItemRobinetterie() {\n      this.appartements.inter_robinetterie.pop();\n      this.appartements.repa_robinetterie.pop();\n    },\n    removeItemRobinetterieC() {\n      this.appartements.inter_robinetterie_c.pop();\n      this.appartements.repa_robinetterie_c.pop();\n    },\n    removeItem(index, type) {\n      this['inter_' + type].splice(index, 1);\n      this.saveFormData();\n    },\n    submitForm() {\n      this.getFormData();\n    }\n  }\n};","map":{"version":3,"mappings":";AAuTA,SAASA,GAAG,EAAEC,GAAE,QAAS,YAAY;AAErC,eAAe;EACdC,IAAI,EAAE,SAAS;EACfC,IAAI,GAAG;IACN,OAAO;MACNC,MAAM,EAAE,EAAE;MACVC,KAAK,EAAE,EAAE;MACTC,YAAY,EAAE;QACbC,GAAG,EAAE,EAAE;QACPC,GAAG,EAAE,EAAE;QACPC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,EAAE;QACTC,KAAK,EAAE,EAAE;QACTC,KAAK,EAAE,EAAE;QACTC,WAAW,EAAE,EAAE;QACfC,WAAW,EAAE,EAAE;QACfC,gBAAgB,EAAE,KAAK;QACvBC,gBAAgB,EAAE,KAAK;QACvBC,cAAc,EAAE,KAAK;QACrBC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,CACN;UACCC,UAAU,EAAE,EAAE;UACdC,UAAU,EAAE,EAAE;UACdC,YAAY,EAAE,EAAE;UAChBC,SAAS,EAAE;QACZ,CAAC,CACD;QACDC,QAAQ,EAAE,KAAK;QACfC,SAAS,EAAE,KAAK;QAChBC,QAAQ,EAAE;MACX;IACD,CAAC;EACF,CAAC;EACDC,OAAO,GAAG;IACT,IAAIC,GAAE,GAAIC,MAAM,CAACC,QAAQ,CAACC,IAAI,CAACC,KAAK,CAAC,GAAG,CAAC;IACzC,IAAIC,IAAG,GAAIL,GAAG,CAAC,CAAC,CAAC,CAACI,KAAK,CAAC,GAAG,CAAC;IAC5B,IAAIE,OAAM,GAAI,CAAC,CAAC;IAChB,IAAIC,GAAE,GAAI,EAAE;IACZF,IAAI,CAACG,OAAO,CAAC,UAASC,CAAC,EAAE;MACzBF,GAAE,GAAIE,CAAC,CAACL,KAAK,CAAC,GAAG,CAAC;MAClB,IAAGG,GAAG,CAACG,MAAK,IAAK,CAAC,EACjBJ,OAAO,CAACC,GAAG,CAAC,CAAC,CAAC,IAAIA,GAAG,CAAC,CAAC,CAAC;IACzB,CAAC,CAAC;IACF,IAAII,OAAM,GAAIL,OAAO,CAACM,KAAK;IAC3B,IAAI,CAACpC,MAAK,GAAImC,OAAO;IACrB,IAAI,CAAClC,KAAI,GAAI,IAAI,CAACoC,MAAM,CAACC,MAAM,CAACC,EAAE;;IAElC;IACA;IACA;;IAEA,IAAI,CAACC,WAAW,EAAE;IAClB;EACD,CAAC;;EACA;EACA;EACA;EACAC,OAAO,EAAE;IACP,MAAMC,YAAY,GAAG;MACvB;MACA,IAAIC,MAAK,GAAI,IAAI,CAACC,KAAK;MACvB;MACAD,MAAK,GAAIE,IAAI,CAACC,SAAS,CAACH,MAAM,CAAC;MAC/B,IAAII,IAAG,GAAIF,IAAI,CAACG,KAAK,CAACL,MAAM,CAAC;MAC7BM