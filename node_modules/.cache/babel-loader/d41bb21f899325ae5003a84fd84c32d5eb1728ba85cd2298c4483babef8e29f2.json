',\n            DP_observations: '',\n            DP_remarqueContractuelle: '',\n            DP_remarqueInter: '',\n            temps_passe: '',\n\t\t\tstatut: '',\n\t\t\traf_devis: false,\n\t\t\tacces_porte: '',\n            inter_option: [],\n\t\t\tinter: [\n\t\t\t\t{\n\t\t\t\t\tinter_type: '',\n\t\t\t\t\tinter_lieu: '',\n\t\t\t\t\tinter_presta: '',\n\t\t\t\t\tinter_qty: '',\n\t\t\t\t},\n\t\t\t],\n\t\t};\n\t},\n    mounted () {\n\t\tlet uri = window.location.href.split('?');\n\t\tlet vars = uri[1].split('&');\n\t\tlet getVars = {};\n\t\tlet tmp = '';\n\t\tvars.forEach(function(v) {\n\t\ttmp = v.split('=');\n\t\tif(tmp.length == 2)\n\t\t\tgetVars[tmp[0]] = tmp[1];\n\t\t});\n\t\tlet item_id = getVars['item_id'];\n\t\tthis.dp_id = item_id;\n\t\taxios.get('http://192.168.1.143:8000/mission_dp?item_id='+item_id).then((response) => {\n\t\t\tthis.resp = response.data;\n\t\t\tconsole.log(this.resp);\n\t\t});\n\t\taxios.get('http://192.168.1.143:8000/dp_occ?dp_id='+item_id).then((response) => {\n\t\t\tthis.occ = response.data;\n\t\t});\n\t\tthis.getFormData();\n\t\t// window.addEventListener('beforeunload', this.saveFormData);\n    },\n\n\tbeforeUnmount() {\n\t\t// window.removeEventListener('beforeunload', this.saveFormData);\n\t},\n    methods: {\n\t\tgetFormValues() {\n\t\t\tconst content = {\n\t\t\t\t\t\t\t\tDP_id: this.dp_id,\n\t\t\t\t\t\t\t\tDP_remarqueContractuelle: this.DP_remarqueContractuelle,\n\t\t\t\t\t\t\t\tDP_remarqueInter: this.DP_remarqueInter,\n\t\t\t\t\t\t\t\tDP_observations: this.DP_observations,\n\t\t\t\t\t\t\t\ttemps_passe: this.temps_passe,\n\t\t\t\t\t\t\t\tstatut: this.statut,\n\t\t\t\t\t\t\t\tacces_porte: this.acces_porte,\n\t\t\t\t\t\t\t\t//bat, etag, port etc qui seront modifiables dans header\n\t\t\t\t\t\t\t\t//nom de l'occupant\n\t\t\t\t\t\t\t\traf_devis: this.raf_devis,\n\t\t\t\t\t\t\t\tinter: this.inter,\n\t\t\t\t\t\t\t};\n\t\t\tconsole.log(content);\n\t\t\t// axios.post(\"http://172.20.10.2:8000/submit_dp\", content).then((response) => {\n\t\