artements[i - 1]) {\n          await set('VG-' + this.vg_id + '-' + i, test.appartements[i - 1]);\n          i = i + 1;\n        }\n      } else {\n        await set('VG-' + this.vg_id + '-' + occ, test.appartements[occ - 1]);\n      }\n      await set('VG-' + this.vg_id, test);\n    },\n    async getFormData() {\n      let i = 1;\n      let data = [];\n      if ((await get('VG-' + this.vg_id + '-' + i)) !== undefined) {\n        while ((await get('VG-' + this.vg_id + '-' + i)) !== undefined) {\n          let savedData = await get('VG-' + this.vg_id + '-' + i);\n          data[i - 1] = savedData;\n          i = i + 1;\n        }\n        this.appartements = data;\n      }\n      if ((await get('VG-' + this.vg_id)) !== undefined) {\n        let savedData = await get('VG-' + this.vg_id);\n        this.compteurs = savedData.compteurs;\n        this.remarque_tech = savedData.remarque_tech;\n        this.temps_passe = savedData.temps_passe;\n      }\n    },\n    addItemAppartements() {\n      var current = this.appartements[this.appartements.length - 1];\n      this.appartements.push({\n        num: Number(current.num) + 1,\n        bat: current.bat,\n        escalier: current.escalier,\n        etage: current.etage,\n        porte: '',\n        suivi: '',\n        remarque_inter: '',\n        checkbox_cloture: false,\n        checkbox_suspens: false,\n        checkbox_devis: false,\n        remarque: '',\n        inter: [{\n          inter_type: '',\n          inter_lieu: '',\n          inter_presta: '',\n          inter_qty: ''\n        }],\n        verif_ra: false,\n        verif_rob: false,\n        verif_wc: false\n      });\n      this.saveFormData();\n    },\n    addItemCompteurs() {\n      //mettre type en arg\n      this.compteurs.push({\n        numero: '',\n        type: '',\n        date: '',\n        index: ''\n      });\n      this.saveFormData();\n    },\n    removeItem(index, type) {\n      this['inter_' + type].splice(index, 1);\n      this.saveFormData();\n    },\n    removeLine(index) {\n      console.log(index);\n    },\n    submitForm() {\n      const content = {\n        appartements: this.appartements,\n        compteurs: this.compteurs,\n        temps_passe: this.temps_passe,\n        remarque_tech: this.remarque_tech,\n        vg_id: this.vg_id,\n        ctr_code: this.resp.Ctr_code\n      };\n      console.log(content);\n      axios.post(\"http://192.168.119.11:8000/submit_vg\", content).then(response => {\n        console.log(response.data);\n      });\n    }\n  }\n};","map":{"version":3,"mappings":";AACA,SAASA,GAAG,EAAEC,GAAE,QAAS,YAAY;AACrC,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,SAAS;EACfC,IAAI,GAAG;IACL,OAAO;MACLC,WAAW,EAAE,IAAI;MACjBC,IAAI,EAAE,EAAE;MACRC,KAAK,EAAE,EAAE;MACTC,aAAa,EAAE,EAAE;MACjBC,WAAW,EAAE,EAAE;MACfC,SAAS,EAAE,CACT;QACLC,MAAM,EAAE,EAAE;QACVC,IAAI,EAAE,EAAE;QACRC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE;MACF,EACD;MACDC,YAAY,EAAE;MAClB;MACM;QACLC,GAAG,EAAE,GAAG;QACRC,GAAG,EAAE,EAAE;QACPC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,EAAE;QACTC,KAAK,EAAE,EAAE;QACTC,KAAK,EAAE,EAAE;QACTC,cAAc,EAAE,EAAE;QAClBC,gBAAgB,EAAE,KAAK;QACvBC,gBAAgB,EAAE,KAAK;QACvBC,cAAc,EAAE,KAAK;QACrBC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,CACN;UACCC,UAAU,EAAE,EAAE;UACdC,UAAU,EAAE,EAAE;UACdC,YAAY,EAAE,EAAE;UAChBC,SAAS,EAAE;QACZ,CAAC,CACD;QACDC,QAAQ,EAAE,KAAK;QACfC,SAAS,EAAE,KAAK;QAChBC,QAAQ,EAAE;MACX;IAEE,CAAC;EACH,CAAC;EACDC,OAAO,GAAG;IACR,IAAIC,GAAE,GAAIC,MAAM,CAACC,QAAQ,CAACC,IAAI,CAACC,KAAK,CAAC,GAAG,CAAC;IAC5C,IAAIC,IAAG,GAAIL,GAAG,CAAC,CAAC,CAAC,CAACI,KAAK,CAAC,GAAG,CAAC;IAC5B,IAAIE,OAAM,GAAI,CAAC,CAAC;IAChB,IAAIC,GAAE,GAAI,EAAE;IACZF,IAAI,CAACG,OAAO,CAAC,UAASC,CAAC,EAAE;MACzBF,GAAE,GAAIE,CAAC,CAACL,KAAK,CAAC,GAAG,CAAC;MAClB,IAAGG,GAAG,CAACG,MAAK,IAAK,CAAC,EACjBJ,OAAO,CAACC,GAAG,CAAC,CAAC,CAAC,IAAIA,GAAG,CAAC,CAAC,CAAC;IACzB,CAAC,CAAC;IACF,IAAII,OAAM,GAAIL,OAAO,CAAC,SAAS,CAAC;IAChC,IAAI,CAACnC,KAAI,GAAIwC,OAAO;IACjB7C,KAAK,CAACD,GAAG,CAAC,gDAAgD,GAAC8C,OAAO,CAAC,CAACC,IAAI,CAAEC,QAAQ,IAAK;MACzF,IAAI,CAAC3C,IAAG,GAAI2C,QAAQ,CAAC7C