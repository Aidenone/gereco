    async saveFormData(occ) {\n      occ = occ + 1;\n      //Data from table + form\n      var result = this.$data;\n      console.log(result);\n      var test = JSON.stringify(result);\n      console.log(test);\n      test = JSON.parse(test);\n      if (isNaN(occ)) {\n        let i = 1;\n        while (await get('VG-' + this.vg_id + '-' + i)) {\n          await del('VG-' + this.vg_id + '-' + i);\n          i = i + 1;\n        }\n        i = 1;\n        while (test.appartements[i - 1]) {\n          await set('VG-' + this.vg_id + '-' + i, test.appartements[i - 1]);\n          i = i + 1;\n        }\n      } else {\n        await set('VG-' + this.vg_id + '-' + occ, test.appartements[occ - 1]);\n      }\n      await set('VG-' + this.vg_id, test);\n      await set('test', inter_option);\n    },\n    async getFormData() {\n      let i = 1;\n      let data = [];\n      if ((await get('VG-' + this.vg_id + '-' + i)) !== undefined) {\n        while ((await get('VG-' + this.vg_id + '-' + i)) !== undefined) {\n          let savedData = await get('VG-' + this.vg_id + '-' + i);\n          data[i - 1] = savedData;\n          i = i + 1;\n        }\n        this.appartements = data;\n      }\n      if ((await get('VG-' + this.vg_id)) !== undefined) {\n        let savedData = await get('VG-' + this.vg_id);\n        this.compteurs = savedData.compteurs;\n        this.compte_rendu = savedData.compte_rendu;\n        this.temps_passe = savedData.temps_passe;\n      }\n      this.saveFormData();\n    },\n    addItemAppartements() {\n      var current = this.appartements[this.appartements.length - 1];\n      var curr_num = current.num;\n      this.appartements.push({\n        num: Number(curr_num) + 1,\n        bat: current.bat,\n        nom: current.nom,\n        escalier: current.escalier,\n        etage: current.etage,\n        porte: '',\n        suivi: '',\n        remarque_inter: '',\n        checkbox_cloture: false,\n        checkbox_suspens: false,\n        checkbox_devis: false,\n        remarque: '',\n        inter: [{\n          inter_type: '',\n          inter_lieu: '',\n          inter_presta: '',\n          inter_qty: ''\n        }],\n        verif_ra: false,\n        verif_rob: false,\n        verif_wc: false,\n        verif_gen: false\n      });\n      this.saveFormData();\n    },\n    addItemCompteurs() {\n      //mettre type en arg\n      this.compteurs.push({\n        numero: '',\n        type: '',\n        date: '',\n        index: ''\n      });\n      this.saveFormData();\n    },\n    removeItem(index, type) {\n      this['inter_' + type].splice(index, 1);\n      this.saveFormData();\n    },\n    removeLine(index) {\n      if (confirm(\"Voulez-vous supprimer cette ligne ?\")) {\n        this.appartements.splice(index, 1);\n        this.saveFormData();\n      }\n    },\n    removeCompteur(index) {\n      if (confirm(\"Voulez-vous supprimer cette ligne ?\")) {\n        this.compteurs.splice(index, 1);\n        this.saveFormData();\n      }\n    },\n    submitForm() {\n      const content = {\n        appartements: this.appartements,\n        compteurs: this.compteurs,\n        temps_passe: this.temps_passe,\n        compte_rendu: this.compte_rendu,\n        vg_id: this.vg_id,\n        remarque: this.remarque,\n        ctr_code: this.resp.Ctr_code,\n        ctr_nature: this.resp.Ctr_nature\n      };\n      // console.log(content);\n      console.log(content);\n      axios.post(\"http://192.168.73.11:8000/submit_vg\", content).then(response => {\n        console.log(response.data);\n      });\n    }\n  }\n};","map":{"version":3,"mappings":";AACA,SAASA,GAAG,EAAEC,GAAG,EAAEC,GAAE,QAAS,YAAY;AAC1C,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,SAAS;EACfC,IAAI,GAAG;IACL,OAAO;MACLC,WAAW,EAAE,IAAI;MACjBC,MAAM,EAAE,IAAI,CAACA,MAAM;MACnBC,IAAI,EAAE,EAAE;MACRC,KAAK,EAAE,EAAE;MACTC,YAAY,EAAE,EAAE;MAChBC,QAAQ,EAAE,EAAE;MACZC,WAAW,EAAE,EAAE;MACfC,YAAY,EAAE,EAAE;MAChBC,SAAS,EAAE,CACT;QACLC,MAAM,EAAE,EAAE;QACVC,IAAI,EAAE,EAAE;QACRC,IAAI,EAAE,EAAE;